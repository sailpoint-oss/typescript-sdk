{
  "name": "sailpoint-api-client",
  "version": "1.7.6",
  "description": "Official library for using the SailPoint API",
  "author": "SailPoint DevOps",
  "repository": {
    "type": "git",
    "url": "https://github.com/sailpoint-oss/typescript-sdk.git"
  },
  "keywords": [
    "axios",
    "typescript",
    "sdk",
    "sailpoint"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sailpoint-oss/typescript-sdk/issues"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/",
    "prepare": "npm run build",
    "test": "jest --ci"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "axios-retry": "^3.4.0",
    "js-yaml": "^4.1.0",
    "proxy-agent": "^6.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^12.11.5",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.0"
  },
  "overrides": {
    "@babel/helpers": "^7.26.10",
    "@babel/runtime": "^7.26.10",
    "form-data": "^4.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "clearMocks": true,
    "collectCoverage": false,
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/beta/",
      "<rootDir>/generic/",
      "<rootDir>/v3/",
      "<rootDir>/v2024/",
      "<rootDir>/v2025/",
      "<rootDir>/v2026/",
      "<rootDir>/dist/"
    ]
  }
}
